{
  "name": "parallel-intercepting-catchall-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:generate": "prisma generate && next dev",
    "build": "prisma generate && next build",
    "build:with-prisma": "npm run update-schema && next build",
    "start": "next start",
    "lint": "next lint",
    "update-schema:seed": "prisma generate && prisma db push && prisma db seed",
    "update-schema": "prisma generate && prisma db push",
    "reset-schema:seed": "prisma generate && prisma db push --force-reset && prisma db seed",
    "reset-schema": "prisma generate && prisma db push --force-reset"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
